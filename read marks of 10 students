#include <stdio.h>
int main(){
int arr[10],i;
for(i=0;i<10;i++){
printf("Enter the marks of student %d\n",i+1);
scanf("%d",arr[i]);
}
int a,b,c,d,e,f,g,h,i1,j;
a=0;b=0;c=0;d=0;e=0;f=0;g=0;h=0;i1=0;j=0;
for(i=0;i<10;i++)
{
if(arr[i]<10)
{a++; }
else if(arr[i]<20)
{b++; }
else if(arr[i]<30)
{c++ ;}
else if(arr[i]<40)
{d++ ;}
else if(arr[i]<50)
{e++ ;}
else if (arr[i]<60)
{f++ ;}
else if(arr[i]<70)
{g++ ;}
else if(arr[i]<80)
{h++ ;}
else if (arr[i]<90)
{i1++ ;}
else if(arr[i]<100)
{j++ ;}
else
printf("Invalid entry");
}
printf("0 to 10 :%d\n",a);
printf("10 to 20 :%d\n",b);
printf("20 to 30 :%d\n",c);
printf("30 to 40 :%d\n",d);
printf("40 to 50 :%d\n",e);
printf("50 to 60 :%d\n",f);
printf("60 to 70 :%d\n",g);
printf("70 to 80 :%d\n",h);
printf("80 to 90 :%d\n",i);
printf("90 to 100 :%sd\n",j);
}

