#include <stdio.h>
int main(){
int sal,int-sal;
char gen;
float new-sal;
printf("Enter the salary \n");
printf("Enter M for male and F for female\n");
scanf("%d %c",&sal,&gen):
int-sal=sal;
if(gen=='F'){
new-sal=sal+(sal*0.1);
}
else if(gen=='M'){
new-sal=sal+(sal*0.05);
}
if(int-sal<10000){
if(gen=='M')
new-sal=sal+(sal*.07):
else if(gen=='F')
new-sal=sal+(sal*0.12);
}
printf("The new salary is %.2f",new-sal):
}
